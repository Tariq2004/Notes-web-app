<%- include("partials/header"); -%>

<form class="" action="/publish" method="post">
  <div class="form-group">
    <label for="sminput">Title</label>
      <input class="form-control" type="text" name="title" id="sminput" placeholder="Add tilte to your post" autocomplete="off">
  </div>

  <div class="form-group big">
    <label for="lginput">Post</label>
    <textarea class="form-control" name="post" rows="3" id="lginput" cols="80" placeholder="Add a post" autocomplete="off"></textarea>
  </div>
<button class="add2 btn btn-outline-info" type="submit" name="button">+</button>
</form>

